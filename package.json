{
  "private": true,
  "sideEffects": false,
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "clean": "rimraf node_modules .cache .mf build dist functions",
    "generate-client": "prisma generate --data-proxy",
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler",
    "postinstall": "remix-esbuild-override && remix setup cloudflare && npm run generate-client"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.5",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@prisma/client": "^4.4.0",
    "@remix-run/cloudflare-pages": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "cross-env": "^7.0.3",
    "framer-motion": "^7.5.3",
    "prisma": "^4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "remix": "^1.7.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.17.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.25.0",
    "npm-run-all": "^4.1.5",
    "remix-esbuild-override": "^3.0.4",
    "typescript": "^4.8.4",
    "wrangler": "2.1.11"
  },
  "engines": {
    "node": ">=16"
  }
}
